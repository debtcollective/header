{
  "name": "@debtcollective/header",
  "version": "0.0.0-development",
  "private": false,
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.8",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "commit": "npx git-cz",
    "eject": "react-scripts eject",
    "format:all": "prettier .prettierrc --write '**/*.*(js|jsx|md|mdx)'",
    "lint": "eslint ./",
    "pretty-quick": "pretty-quick",
    "semantic-release": "semantic-release",
    "start": "PORT=8080 react-scripts start",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/debtcollective/header.git"
  },
  "resolutions": {
    "ansi-styles": "^3.2.0"
  },
  "devDependencies": {
    "docz": "0.13.7",
    "docz-theme-default": "0.13.7",
    "dom-testing-library": "3.18.2",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-react": "7.12.4",
    "faker": "4.1.0",
    "flow-bin": "0.95.1",
    "husky": "1.3.1",
    "jest-dom": "3.1.3",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "react-testing-library": "6.0.4",
    "semantic-release": "15.13.3",
    "user-event": "1.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "post-commit": "git update-index --again",
      "pre-push": "CI=true yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "flow focus-check",
      "eslint --fix",
      "git add"
    ]
  }
}
