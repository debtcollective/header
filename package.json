{
  "name": "@debtcollective/header",
  "version": "0.0.0-development",
  "private": false,
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore src/lib/**/__tests__,src/lib/**/__fixtures__",
    "commit": "npx git-cz",
    "eject": "react-scripts eject",
    "format:all": "prettier .prettierrc --write '**/*.*(js|jsx|md|mdx)'",
    "lint": "eslint ./",
    "pretty-quick": "pretty-quick",
    "semantic-release": "semantic-release",
    "start": "PORT=8080 react-scripts start",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "defaults"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/debtcollective/header.git"
  },
  "resolutions": {
    "ansi-styles": "^3.2.0",
    "babel-jest": "24.7.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-popper": "^1.3.3",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@emotion/core": "10.0.6",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "core-js": "3",
    "docz": "0.13.7",
    "docz-theme-default": "0.13.7",
    "dom-testing-library": "4.0.1",
    "emotion-theming": "10.0.6",
    "eslint-plugin-flowtype": "3.8.1",
    "eslint-plugin-react": "7.13.0",
    "faker": "4.1.0",
    "flow-bin": "0.98.1",
    "husky": "2.2.0",
    "jest-dom": "3.2.1",
    "lint-staged": "8.1.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-popper": "^1.3.3",
    "react-scripts": "3.0.0",
    "react-testing-library": "6.1.2",
    "semantic-release": "15.13.3",
    "styled-components": "^4.2.0",
    "user-event": "2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "post-commit": "git update-index --again",
      "pre-push": "CI=true yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "flow focus-check",
      "eslint --fix",
      "git add"
    ]
  }
}
